  "scripts": {
    "test": "npm run eslint && npm run jest",
    "debugtest": "node --inspect-brk node_modules/.bin/jest --watch --runInBand",
    "build": "npm run build-styles && npm run babel-es5 && npm run babel-module",
    "babel-es5": "babel src/ --out-dir dist/js --no-babelrc --ignore \"tests/*.js, src/js\"",
    "babel-module": "babel src/ --out-dir module/js --configFile=false --ignore \"tests/*.js, src/js\"",
    "codecov": "codecov",
    "jest": "jest --config jest.config.js --no-cache",
    "jest:watch": "jest --config jest.config.js --watch",
    "lint": "git diff origin/devel... --name-only --relative -- '*.js' | tr '\n' ' ' | xargs yarn eslint --ext .js --fix",
    "lint:nofix": "git diff origin/devel... --name-only --relative -- '*.js' | tr '\n' ' ' | xargs yarn eslint --ext .js",
    "eslint": "eslint --ext .jsx --ext .js . --max-warnings 0",
    "preversion": "git checkout master && git pull --rebase=false",
    "postversion": "npm run postversion:publish && npm run postversion:update-devel",
    "postversion:publish": "git push --follow-tags",
    "postversion:update-devel": "git checkout devel && git pull && git merge master && git push origin devel && git checkout master",
    "storybook": "yarn generate-tokens:watch & yarn csswhat:watch & start-storybook -p 9009",
    "build-storybook": "yarn generate-tokens && yarn csswhat && build-storybook -c .storybook -o ./storybook",
    "build-styles": "yarn csswhat && ./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack --watch",
    "csswhat": "yarn csswhat-css && yarn csswhat-json",
    "csswhat-css": "node-sass-chokidar src/oui/oui.scss -o ./data/csswhat",
    "csswhat-json": "node node_modules/csswhat/csswhat.js --in=./data/csswhat/oui.css --out=./data/csswhat/oui.css.json",
    "csswhat:watch": "yarn csswhat-json:watch & yarn csswhat-css:watch",
    "csswhat-json:watch": "onchange 'data/csswhat/oui.css' -- yarn csswhat-json",
    "csswhat-css:watch": "node-sass-chokidar src/oui/oui.scss --watch -o ./data/csswhat",
    "generate-tokens": "node src/tokens/index.js",
    "generate-tokens:watch": "onchange 'src/tokens/src/**/*.json' -- yarn generate-tokens"
  },
